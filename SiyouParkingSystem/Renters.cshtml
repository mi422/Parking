                    function validation() {
                        var valid = true;
                        var name = document.getElementById('PostName').value;
                        var phone = document.getElementById('postphone').value;
                        var address = document.getElementById('postaddress').value;
                        var identit = document.getElementById('postqrcode').value;
                        

                        if (name == "") {
                            document.getElementById('NameErr').innerHTML = "please fill the field";
                            valid = false;
                        }
                        else if ((name.length <= 5) || (name.length > 20)) {
                            document.getElementById('NameErr').innerHTML = "Full name is between 5 & 20";
                            valid = false;
                        }
                        else if (!isNaN(name)) {
                            document.getElementById('NameErr').innerHTML = "only characters are allowed";
                            valid = false;
                        };
                        if (phone == "") {
                            document.getElementById('PhoneErr').innerHTML = "please fill the field";
                            valid = false;
                        }
                        else if (isNaN(phone)) {
                            document.getElementById('PhoneErr').innerHTML = "phone must have only numbers";
                            valid = false;
                        }
                        else if (phone.length != 8) {
                            document.getElementById('PhoneErr').innerHTML = "phone must have only 8 numbers";
                            valid = false;
                        };
                        if (address == "") {
                            document.getElementById('AdrErr').innerHTML = "please fill the field";
                            valid = false;
                        }
                        else if ((address.length <= 3) || (address.length > 10)) {
                            document.getElementById('AdrErr').innerHTML = "please insert a valid address";
                            valid = false;
                        }
                        if (identit == "") {
                            document.getElementById('IdentityErr').innerHTML = "please fill the field";
                            valid = false;
                        }
                    }
