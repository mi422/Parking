                width: 140,
                height: 140,
                colorDark: "#000000",
                colorLight: "#ffffff",
                correctLevel: QRCode.CorrectLevel.H
            });

            function GenerateQR() {

                var data = qrdata.value;
                if (data == "") {
                    document.getElementById('IdentityErr').innerHTML = "please fill the field";
                    valid = false;
                }
                else if (data.length != 8) {
                    document.getElementById('IdentityErr').innerHTML = "please insert a valid identity number";
                    valid = false;
                }
                else if (isNaN(data)) {
                    document.getElementById('IdentityErr').innerHTML = "please insert a valid identity number";
                    valid = false;
                }
                else {

                    qrcodee.makeCode(data);
                    document.getElementById("IdentityErr").innerHTML = "";
                }
            }
        </script>
