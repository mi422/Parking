<script type="text/javascript">
    function RenterList() {
        // Call Web API to get a list of Admins
        $.ajax({
            url: 'api/Renters',
            type: 'GET',
            dataType: 'json',
            success: function (Renters) {
                RenterListSuccess(Renters);
            },
            error: function (request, message, error) {
                handleException(request, message, error);
            }
        });
    }
    function RenterListSuccess(Renters) {
        // Iterate over the collection of data
        $.each(Renters, function (index, Renter) {
            // Add a row to the Admin table
            RenterAddRow(Renter);
        });
    }
    function RenterAddRow(Renter) {
        // Check if <tbody> tag exists, add one if not
        if ($("#RenterTable tbody").length == 0) {
            $("#RenterTable").append("<tbody></tbody>");
        }
        // Append row to <table>
        $("#RenterTable tbody").append(
          RenterBuildTableRow(Renter));
    }
    function deleteRow(btn) {
        var row = btn.parentNode.parentNode;
        row.parentNode.removeChild(row);
    }
    function RenterBuildTableRow(Renter) {
        var ret =
           "<tr>" +
           "<td>" + Renter.Id + "</td>" +
            "<td>" + Renter.Name + "</td>" +
                "<td>" + Renter.Phone + "</td>"
                 + "<td>" + Renter.Adress + "</td>" 
                 + "<td>" + Renter.QR_code + "</td>" +
                     "<td>" +
