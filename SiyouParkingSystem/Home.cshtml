<script type="text/javascript">
    $(document).ready(function () {
      
        $('#spanUsername').text(localStorage.getItem("userName"));
    });

    function getrole(ctl) {
        var id = $(ctl).data("id");
        $.ajax({
            url: "/api/admins/" + id,
            type: 'GET',
            dataType: 'json',
            success: function (Admin) {
                ConsadminToFields(Admin);
                ConUserGet(ctl);
            },
            error: function (request, message, error) {
                handleException(request, message, error);
            }
        });
    }
    function ConsadminToFields(Admin) {
        $("#consultId").val(Admin.Id);
        $("#consultuserId").val(Admin.UserId);
        $("#ConsName").val(Admin.FirstName);
        $("#ConsSurname").val(Admin.LastName);
        $("#Consphone").val(Admin.Phone);
        $("#Consaddress").val(Admin.Address);
        $("#ConBirth").val(Admin.Birth_date);
    }
    function ConUserGet(ctl) {
        var id = $("#consultuserId").val();
        $.ajax({
            url: "api/Register/Get/" + id,
            type: 'GET',
            dataType: 'json',
            success: function (User) {
                ConsUserToFields(User);
            },
            error: function (request, message, error) {
                handleException(request, message, error);
            }
        });
    }
    function remove() {

        window.localStorage.clear();
    }
 </script>