    <script type="text/javascript">
            function removeWarning() {
                document.getElementById("plateErr").innerHTML = "";
                document.getElementById("modelErr").innerHTML = "";
                document.getElementById("ownerchose").innerHTML = "";
            }
            function validation() {
                var valid = true;
                var plate = document.getElementById('Postplate').value;
                var model = document.getElementById('Postmodel').value;
                var owner = document.getElementById('UserList').value;

                if (plate == "") {
                    document.getElementById('plateErr').innerHTML = "please fill the field";
                    valid = false;
                }
                else if ((plate.length <= 10)) {
                    document.getElementById('plateErr').innerHTML = "plate number is not valid";
                    valid = false;
                };
                if (model == "") {
                    document.getElementById('modelErr').innerHTML = "please fill the field";
                    valid = false;
                }
                else if ((model.length <= 2) || (model.length > 20)) {
                    document.getElementById('modelErr').innerHTML = "model is not valid";
                    valid = false;
                };
                if (owner == "") {
                    document.getElementById('ownerchose').innerHTML = "please fill the field";
                    valid = false;
                }
            }
        </script>
